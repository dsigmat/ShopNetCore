@using ShopNetCore.Data.Models
@model IEnumerable<Car>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>List</title>
</head>
<body>
    <h2>Все автомобили</h2>
    @{ 
        foreach (var car in Model)
        {
    <div>
        <h3>Модель: @car.Name</h3>
        <h4>Модель: @car.Price.ToString("c")</h4>
   </div>
        }
    }
</body>
</html>
